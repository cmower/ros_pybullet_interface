<launch>

  <!-- ROS-Pybullet interface node simulator -->
  <node pkg="ros_pybullet_interface" name="ros_pybullet_interface_node" type="ros_pybullet_interface_node.py" output="screen">

    <!-- Config file for Pybullet -->
    <param name="pybullet_config" value="{ros_pybullet_interface_examples}/configs/pybullet_basic_example.yaml" type="str"/>

    <!-- Node frequency -->
    <param name="ros_node_freq" value="100" type="int"/>

    <!-- Simulated robot -->
    <rosparam param="robot_config_file_names">
      [
      "{ros_pybullet_interface_examples}/configs/kuka.yaml",
      "{ros_pybullet_interface_examples}/configs/iiwa.yaml",
      "{ros_pybullet_interface_examples}/configs/nextage.yaml",
      "{ros_pybullet_interface_examples}/configs/talos.yaml",
      "{ros_pybullet_interface_examples}/configs/valkyrie.yaml",
      "{ros_pybullet_interface_examples}/configs/anymal.yaml",
      ]
    </rosparam>

    <!-- Visual robot -->
    <rosparam param="visual_robot_config_file_names">
      [
      ]
    </rosparam>

    <rosparam param="dynamic_object_config_file_names">
      [
      ]
    </rosparam>

    <rosparam param="collision_object_config_file_names">
      [
      ]
    </rosparam>

    <rosparam param="visual_object_config_file_names">
      [
      ]
    </rosparam>

  </node>

  <!-- Example node -->
  <node pkg="ros_pybullet_interface_examples" name="basic_talos_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="talos"/>
  </node>
  <node pkg="ros_pybullet_interface_examples" name="basic_lwr_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="LWR"/>
  </node>
  <node pkg="ros_pybullet_interface_examples" name="basic_iiwa_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="iiwa"/>
  </node>
  <node pkg="ros_pybullet_interface_examples" name="basic_nextage_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="nextage"/>
  </node>
  <node pkg="ros_pybullet_interface_examples" name="basic_valkyrie_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="valkyrie"/>
  </node>
  <node pkg="ros_pybullet_interface_examples" name="basic_anymal_example_node" type="basic_robot_example_node.py">
    <param name="robot_name" type="str" value="anymal"/>
  </node>

  <!-- Static tf frames -->

  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_lwr_base_tf_broadcaster"
	args="0.5 0.0 0 0 0 0 rpbi/world lwr_base"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_iiwa_base_tf_broadcaster"
	args="1.75 0.0 0 0 0 0 rpbi/world iiwa_base"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_nextage_base_tf_broadcaster"
	args="3 0.0 0.85 -1.57 0 0 rpbi/world nextage_base"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_talos_base_tf_broadcaster"
	args="4.5 0.0 1.0 -1.57 0 0 rpbi/world talos_base"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_valkyrie_base_tf_broadcaster"
	args="6 0.0 1.0 -1.57 0 0 rpbi/world valkyrie_base"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="simulated_anymal_base_tf_broadcaster"
	args="-1 0.0 0.5 -1.57 0 0 rpbi/world anymal_base"/>

</launch>
