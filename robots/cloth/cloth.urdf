<?xml version="1.0" encoding="utf-8"?>


<robot name="cloth">

<material name="color1">
    <color rgba="0.8 0.3 0.3 1"/>
</material>

<material name="color2">
    <color rgba="0.3 0.3 0.3 1"/>
</material>

<material name="color3">
    <color rgba="0.3 0.7 0.3 1"/>
</material>
<!--To open this model with Gazebo replace the null masses, inertias and dimensions of the 'fake links f1 and f2'-->
    <!--LINKS-->


    <!-- <link name="world">
      <inertial>
        <mass value="0"/>
        <inertia ixx="0.0" ixy="0" ixz="0" iyy="0.0" iyz="0" izz="0.0"/>
      </inertial>
    </link> -->


	<link name="Cyl1">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0  -0.  0" rpy="0 0 0" />
            <inertia ixx="0.1" iyy="0.1" izz="0.02" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0    -0.0          0" rpy="0 0 0" />
            <geometry>
              <cylinder length="0.12" radius="0.02"/>
            </geometry>
            <material name="color"/>
        </visual>

        <collision>
			<!--box origin wrt jRightElbow. RPY rotated of pi/2.-->
            <origin xyz="0 -0. 0" rpy="0 0 0" />
            <geometry>
              <cylinder length="0.12" radius="0.02"/>
            </geometry>
        </collision>

    </link>

    <link name="Cyl2">
          <inertial>
              <mass value="0.1"/>
              <origin xyz="0  -0.  0" rpy="0 0 0" />
              <inertia ixx="0.1" iyy="0.1" izz="0.02" ixy="0" ixz="0" iyz="0"/>
          </inertial>

          <visual>
              <origin xyz="0.0 0 -0.0" rpy="0 0 0" />
              <geometry>
                  <cylinder length="0.12" radius="0.02"/>
              </geometry>
              <material name="color1"/>
          </visual>

          <collision>
        <!--box origin wrt jRightElbow. RPY rotated of pi/2.-->
              <origin xyz="0.0 0 -0.0" rpy="0 0 0" />
              <geometry>
                  <cylinder length="0.12" radius="0.02"/>
              </geometry>
          </collision>

      </link>

      <link name="Cyl3">
            <inertial>
                <mass value="0.1"/>
                <origin xyz="0  -0.  0" rpy="0 0 0" />
                <inertia ixx="0.1" iyy="0.1" izz="0.02" ixy="0" ixz="0" iyz="0"/>
            </inertial>

            <visual>
                <origin xyz="0.0 0 0.0" rpy="0 0 0" />
                <geometry>
                    <cylinder length="0.12" radius="0.02"/>
                </geometry>
                <material name="color2"/>
            </visual>

            <collision>
          <!--box origin wrt jRightElbow. RPY rotated of pi/2.-->
                <origin xyz="0.0 0 0.0" rpy="0 0 0" />
                <geometry>
                    <cylinder length="0.12" radius="0.02"/>
                </geometry>
            </collision>

        </link>

        <link name="Cyl4">
              <inertial>
                  <mass value="0.1"/>
                  <origin xyz="0  -0.  0" rpy="0 0 0" />
                  <inertia ixx="0.1" iyy="0.1" izz="0.02" ixy="0" ixz="0" iyz="0"/>
              </inertial>

              <visual>
                  <origin xyz="0.0 0 0.0" rpy="0 0 0" />
                  <geometry>
                      <cylinder length="0.12" radius="0.02"/>
                  </geometry>
                  <material name="color3"/>
              </visual>

              <collision>
                  <origin xyz="0.0 0 0.0" rpy="0 0 0" />
                  <geometry>
                      <cylinder length="0.12" radius="0.02"/>
                  </geometry>
              </collision>

          </link>




	<!--Chain from (8) to (11)-->
	  <!-- <joint name="Cyl1_world" type="floating">
        <origin xyz="0. 0.0 -0.0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="Cyl1"/>
        <dynamics damping="1.0" friction="0.0"/>
        <limit effort="6" velocity="1.0" lower="-1000" upper="1000" />
        <axis xyz="1 0 0" />
    </joint> -->

    <joint name="Cyl1_Cyl2" type="revolute">
        <origin xyz="0. 0.06 -0.06" rpy="1.57 0 0"/>
        <parent link="Cyl1"/>
        <child link="Cyl2"/>
        <dynamics damping="1.0" friction="0.0"/>
        <limit effort="10" velocity="1.0" lower="-2.0001" upper="2.0001" />
        <axis xyz="1 0 0" />
    </joint>

    <joint name="Cyl1_Cyl3" type="revolute">
        <origin xyz="0. 0.06 0.06" rpy="1.57 0 0"/>
        <parent link="Cyl1"/>
        <child link="Cyl3"/>
        <dynamics damping="1.0" friction="0.0"/>
        <limit effort="10" velocity="1.0" lower="-2.0001" upper="2.0001" />
        <axis xyz="1 0 0" />
    </joint>
<!-- fixed -->
    <joint name="Cyl3_Cyl4" type="fixed">
        <origin xyz="0. 0.12 -0.0" rpy="-0 0 0"/>
        <parent link="Cyl1"/>
        <child link="Cyl4"/>
    </joint>

</robot>
