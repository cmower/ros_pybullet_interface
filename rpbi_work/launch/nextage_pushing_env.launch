  <!--   <!-\- Visual robot -\-> -->
  <!--   <rosparam param="visual_robot_config_file_names"> -->
  <!--     [ -->
  <!--     ] -->
  <!--   </rosparam> -->

  <!--   <rosparam param="dynamic_object_config_file_names"> -->
  <!--     [ -->
  <!--     "{rpbi_work}/configs/pushing_box.yaml", -->
  <!--     ] -->
  <!--   </rosparam> -->

  <!--   <rosparam param="collision_object_config_file_names"> -->
