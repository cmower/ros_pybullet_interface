<?xml version="0.0" ?>
<robot name="target.urdf">
  <link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="1.0"/>
       <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
		<mesh filename="target.obj" scale="0.1 0.1 0.1"/>
      </geometry>
      <material name="green">
        <color rgba="0.5 0.2 0.3 1"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 	<box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>
</robot>
